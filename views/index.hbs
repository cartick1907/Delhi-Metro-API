
<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
<form>
    <label for="string1">Enter a starting station:</label>
    <input type="text" id="src" name="string1"><br>

    <label for="string2">Enter destination:</label>
    <input type="text" id="dest" name="string2"><br>

    <button type="button" onclick="printStrings()">Print Strings</button>
</form>

<a href="http://localhost:6968/data"><h1>Click here for line data</h1></a>
<script>
    function printStrings() {
        const sc = document.getElementById("src").value;
        const dest = document.getElementById("dest").value;
        console.log(`String 1: ${sc}`);
        console.log(`String 2: ${dest}`);
    }
</script>